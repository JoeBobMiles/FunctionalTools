{
  "name": "@jrm/functional-tools",
  "version": "0.4.0",
  "description": "TypeScript utilities for functional programming.",
  "license": "Unlicense",
  "keywords": [
    "typescript",
    "functional",
    "programming",
    "fp"
  ],
  "author": {
    "name": "Joseph R Miles",
    "email": "joe@jrm.dev",
    "url": "https://jrm.dev"
  },
  "repository": {
    "url": "git+https://github.com/joebobmiles/FunctionalTools",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/joebobmiles/FunctionalTools/issues"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "dist/",
    "package.json"
  ],
  "exports": {
    "./": "./*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "clean": "shx rm -rf ./dist ./types *.js *.d.ts *.js.map *.tsbuildinfo",
    "build:clean": "npm run clean && npm run build",
    "test": "exit 0",
    "postinstall": "npm run build:clean && shx mv dist/* ."
  },
  "dependencies": {
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "shx": "^0.3.3"
  }
}
