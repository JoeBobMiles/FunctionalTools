  "description": "TypeScript utilities for functional programming.",
  "license": "Unlicense",
  "keywords": [
    "typescript",
    "functional",
    "programming",
    "fp"
  ],
  "author": {
    "name": "Joseph R Miles",
    "email": "joe@jrm.dev",
    "url": "https://jrm.dev"
  },
  "repository": {
    "url": "git+https://github.com/joebobmiles/FunctionalTools",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/joebobmiles/FunctionalTools/issues"
  },
  "main": "./index.js",
