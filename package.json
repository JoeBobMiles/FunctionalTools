{
  "name": "@functional-things/functional-things",
  "version": "2.0.1",
  "description": "TypeScript utilities for functional programming.",
  "license": "Unlicense",
  "keywords": [
    "typescript",
    "functional",
    "programming",
    "fp"
  ],
  "author": {
    "name": "Joseph R Miles",
    "email": "joe@jrm.dev",
    "url": "https://jrm.dev"
  },
  "repository": {
    "url": "git+https://github.com/functional-things/functional-things",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/functional-things/functional-things/issues"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "dist/",
    "scripts/",
    "LICENSE",
    "package.json",
    "tsconfig.json"
  ],
  "exports": {
    "./": "./*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "clean": "node ./scripts/clean.js",
    "build:clean": "npm run clean && npm run build",
    "test": "jest",
    "prepublishOnly": "npm run build:clean",
    "postinstall": "node ./scripts/post-install.js",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@amanda-mitchell/semantic-release-npm-multiple": "^2.4.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^26.0.19",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "semantic-release": "^17.3.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "staging",
        "channel": "preview",
        "prerelease": "rc"
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@amanda-mitchell/semantic-release-npm-multiple",
        {
          "registries": {
            "github": {},
            "public": {}
          }
        }
      ],
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
